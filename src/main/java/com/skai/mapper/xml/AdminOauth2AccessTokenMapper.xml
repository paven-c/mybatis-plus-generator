<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skai.mapper.AdminOauth2AccessTokenMapper">
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            id, user_id, user_info, access_token, refresh_token, scopes, expires_time, creator, create_time, updater, update_time, deleted
    </sql>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.skai.entity.AdminOauth2AccessToken">
            <id column="id" property="id"/>
            <result column="user_id" property="userId"/>
            <result column="user_info" property="userInfo"/>
            <result column="access_token" property="accessToken"/>
            <result column="refresh_token" property="refreshToken"/>
            <result column="scopes" property="scopes"/>
            <result column="expires_time" property="expiresTime"/>
            <result column="creator" property="creator"/>
            <result column="create_time" property="createTime"/>
            <result column="updater" property="updater"/>
            <result column="update_time" property="updateTime"/>
            <result column="deleted" property="deleted"/>
    </resultMap>



</mapper>