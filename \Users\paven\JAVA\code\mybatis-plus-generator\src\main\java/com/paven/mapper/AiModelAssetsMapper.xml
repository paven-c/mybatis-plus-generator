<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paven.mapper.AiModelAssetsMapper">
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            id, cloth, gender, backgroud, face, url, deleted
    </sql>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.paven.entity.AiModelAssets">
            <id column="id" property="id"/>
            <result column="cloth" property="cloth"/>
            <result column="gender" property="gender"/>
            <result column="backgroud" property="backgroud"/>
            <result column="face" property="face"/>
            <result column="url" property="url"/>
            <result column="deleted" property="deleted"/>
    </resultMap>



</mapper>