<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aaa.mapper.CreationTaskMapper">
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            id, user_id, creation_id, creation_assets_image_id, task_type, task_no, task_status, finished_time, create_time, update_time, deleted
    </sql>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="aaa.entity.CreationTask">
            <id column="id" property="id"/>
            <result column="user_id" property="userId"/>
            <result column="creation_id" property="creationId"/>
            <result column="creation_assets_image_id" property="creationAssetsImageId"/>
            <result column="task_type" property="taskType"/>
            <result column="task_no" property="taskNo"/>
            <result column="task_status" property="taskStatus"/>
            <result column="finished_time" property="finishedTime"/>
            <result column="create_time" property="createTime"/>
            <result column="update_time" property="updateTime"/>
            <result column="deleted" property="deleted"/>
    </resultMap>



</mapper>