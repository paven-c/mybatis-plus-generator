<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aaa.mapper.CreationArtisticImageMapper">
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
            id, user_id, creation_id, task_id, image_name, image_url, create_time, update_time, deleted
    </sql>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="aaa.entity.CreationArtisticImage">
            <id column="id" property="id"/>
            <result column="user_id" property="userId"/>
            <result column="creation_id" property="creationId"/>
            <result column="task_id" property="taskId"/>
            <result column="image_name" property="imageName"/>
            <result column="image_url" property="imageUrl"/>
            <result column="create_time" property="createTime"/>
            <result column="update_time" property="updateTime"/>
            <result column="deleted" property="deleted"/>
    </resultMap>



</mapper>